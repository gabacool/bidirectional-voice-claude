# Voice input client configuration
# Copy this file to config.yaml and update with your server address

# Parakeet ASR server address (voice input)
server_url: "ws://YOUR_SERVER_IP:8087"

# Audio settings
sample_rate: 16000       # Must match server expectation
chunk_duration: 0.1      # Send audio every 100ms

# TTS server address (voice output)
tts_server_url: "ws://YOUR_SERVER_IP:8088"
